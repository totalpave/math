  "description": "Math library",
  "main": "lib/api.js",
  "types": "lib/api.d.ts",
  "scripts": {
    "lint": "node --max_old_space_size=6144 ./node_modules/eslint/bin/eslint.js --ext .ts '?(src|spec)/**/*.ts' --cache",
